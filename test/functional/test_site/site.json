{
  "baseUrl": "/test_site",
  "faviconPath": "favicon.png",
  "titlePrefix": "",
  "pages": [
    {
      "src": "index.md",
      "title": "Hello World"
    },
    {
      "src": "testAfterSetup.md",
      "title": "Hello World",
      "layout": "testAfterSetup"
    },
    {
      "src": "testEmptyFrontmatter.md",
      "title": "Hello World",
      "layout": "testLayout"
    },
    {
      "src": "testExternalScripts.md",
      "title": "Hello World",
      "layout": "testLayout",
      "externalScripts": [
        "https://cdnjs.cloudflare.com/ajax/libs/mathjax/2.7.5/MathJax.js?config=TeX-MML-AM_CHTML"
      ]
    },
    {
      "src": "testLayouts.md",
      "title": "Hello World"
    },
    {
      "src": "testLayoutsOverride.md",
      "title": "Hello World",
      "layout": "testLayout"
    },
    {
      "glob" : "**/index.md"
    },
    {
      "glob" : "**/test_md_fragment.md"
    }
  ],
  "ignore": [
    "_markbind/layouts/*",
    "_markbind/logs/*",
    "_site/*",
    "site.json",
    "*.md",
    "test.bat",
    "test.sh",
    "testUtil/*",
    "expected/*"
  ],
  "deploy": {
    "message": "Site Update."
  },
  "headingIndexingLevel": 4,
  "plugins" : [
    "testMarkbindPlugin",
    "filterTags"
  ],
  "pluginsContext" : {
    "testMarkbindPlugin" : {
      "pre": "\n\n# Markbind Plugin Pre-render",
      "post": "<p>Node Modules Plugin Post-render</p>"
    },
    "filterTags" : {
      "tags" : ["tag-site-shown", "+tag-site-override-shown", "+tag-site-override-specific-shown"]
    }
  }
}
